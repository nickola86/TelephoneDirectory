<% title t('show_entry') %>
  <p>
    <b><%= t(:first_name) %>:</b>
    <%= @entry.first_name %>
  </p>
  <p>
    <b><%= t(:last_name) %>:</b>
    <%= @entry.last_name %>
  </p>
  <p>
    <b><%= t(:number) %>:</b>
    <%= @entry.number %>
  </p>
  <%=link_to t('back_to_search'), entries_path %>  
  <%=link_to t('edit_entry'), edit_entry_path(@entry.id)%>

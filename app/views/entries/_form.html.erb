<%= form_for @entry do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label t(:first_name) %><br />
    <%= f.text_field :first_name %>
  </p>
  <p>
    <%= f.label t(:last_name) %><br />
    <%= f.text_field :last_name %>
  </p>
  <p>
    <%= f.label t(:number) %><br />
    <%= f.text_field :number %>
  </p>
  <p><%= f.submit %></p>
<% end %>
